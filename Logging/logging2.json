{
  "version": 1,
  "formatters": {
    "simple": {
      "format": "%(asctime)s %(message)s .%(name)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    },
    "info_file_handler": {
      "class": "logging.FileHandler",
      "level": "DEBUG",
      "formatter": "simple",
      "filename": "info.log"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": [
      "console",
      "info_file_handler"
    ]
  }
}